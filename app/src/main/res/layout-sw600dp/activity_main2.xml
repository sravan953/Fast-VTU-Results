    <fragment
        android:name="com.teamidentiti.fastvturesults.Fragments.SemestersFragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"/>

    <FrameLayout
        android:id="@+id/twoPane"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="4"/>
</LinearLayout>
